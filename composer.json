{
    "name": "thecsea/laravel-noredirect-traits",
    "type": "library",
    "keywords": ["laravel", "traits", "no redirect", "json"],
    "description": "The library that allows you to use standard laravel traits without redirect, returning information via JSON and HTTP code, ideal for REST applications",
    "homepage": "http://www.thecsea.it//",
    "minimum-stability": "stable",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "claudio cardinale",
            "email": "cardi@thecsea.it",
            "homepage": "http://thecsea.it"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpdocumentor/phpdocumentor": "*"
    },
    "autoload": {
        "psr-4": {
            "it\\thecsea\\laravel\\noredirect_traits\\": "src/"
        }
    }

}